{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "moduleResolution": "Node"
  },
  "ts-node": {
    "esm": true
  }
}
